<div class="row">
    <div class="col-md-12">
        <h2>Images</h2>
        <hr />
    </div>
    {{#each data}}
    <div class="col-md-4">
        <img class="img-responsive img-thumbnail" src="/images/uploaded/{{this}}">
    </div>
    {{else}}
    <div class="col-md-12 text-center">
        <strong>No Images Available</strong>
    </div>
    {{/each}}

</div>